{
    "name": "sistema-cadastro-consultorio",
    "version": "0.0.1",
    "description": "Sistema de cadastro de clientes para o consultorio",
    "author": "Fernando Koji Yamashiro<fernandokojiyama@gmail.com>",
    "main": "index.js",
    "private": false,
    "engines": {
        "node": ">=10.15.0",
        "npm": ">=6.4.1"
    },
    "scripts": {
        "start": "gulp serve --unhandled-rejections=strict",
        "start:debug": "cross-env DEBUG=sistema-cadastro-consultorio-api:* npm start",
        "build": "gulp",
        "copy": "gulp copy",
        "babel": "gulp babel",
        "lint": "esw *.js server config --color",
        "lint:watch": "npm lint -- --watch",
        "fix-js": "eslint-auto-fix \"src/**/.js\" \"config/**/.js\" \"config/*.js\"",
        "beautify": "es-beautifier -u -c .eslintrc.js *.js server config",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "cross-env NODE_ENV=test ./node_modules/.bin/istanbul cover _jest -- --ui bdd --reporter spec --colors --compilers js:babel-core/register server/tests --recursive",
        "test:check-coverage": "npm test:coverage && istanbul check-coverage",
        "report-coverage": "coveralls < ./coverage/lcov.info"
    },
    "keywords": [
        "express",
        "node",
        "node.js",
        "mongodb",
        "mongoose",
        "es6",
        "jest",
        "istanbul",
        "REST",
        "API",
        "boilerplate"
    ],
    "jest": {
        "automock": false,
        "bail": false,
        "verbose": true,
        "setupFiles": [
            "./jest-setup.js"
        ],
        "testMatch": [
            "**/Tests/**/*.js",
            "**/?(*.)(spec|test|integration).js?(x)"
        ],
        "testPathIgnorePatterns": [
            "/node_modules/",
            "/coverage/",
            "/dist/"
        ],
        "testEnvironment": "node"
    },
    "dependencies": {
        "@sendgrid/mail": "^7.2.4",
        "axios": "^0.21.1",
        "bcryptjs": "^2.4.3",
        "bluebird": "3.5.5",
        "body-parser": "1.19.0",
        "compression": "1.7.4",
        "cookie-parser": "1.4.4",
        "cors": "2.8.5",
        "crypto-js": "^4.0.0",
        "date-fns": "^2.17.0",
        "debug": "^4.1.1",
        "del": "^5.0.0",
        "dompurify": "^2.2.6",
        "dotenv": "^8.0.0",
        "express": "4.17.1",
        "express-jwt": "5.3.1",
        "express-validation": "^3.0.6",
        "express-winston": "3.3.0",
        "fs-extra": "^9.1.0",
        "gulp": "4.0.2",
        "gulp-load-plugins": "^2.0.1",
        "helmet": "3.20.0",
        "http-status": "^1.3.2",
        "ioredis": "^4.23.0",
        "joi": "14.3.1",
        "jsonwebtoken": "8.5.1",
        "lodash": "^4.17.20",
        "method-override": "^3.0.0",
        "moment-timezone": "^0.5.33",
        "multer": "^1.4.2",
        "node-cron": "^2.0.3",
        "nodemailer": "^6.5.0",
        "pg": "7.12.1",
        "pg-hstore": "2.3.3",
        "sequelize": "5.15.1",
        "socket.io": "^3.1.2",
        "socket.io-redis": "^6.0.1",
        "supertest": "4.0.2",
        "winston": "3.2.1"
    },
    "devDependencies": {
        "@babel/core": "^7.2.2",
        "@babel/plugin-proposal-export-default-from": "^7.2.0",
        "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
        "@babel/plugin-proposal-function-bind": "^7.2.0",
        "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
        "@babel/plugin-syntax-dynamic-import": "^7.2.0",
        "@babel/plugin-syntax-export-namespace-from": "^7.2.0",
        "@babel/plugin-transform-modules-commonjs": "^7.5.0",
        "@babel/preset-env": "^7.2.0",
        "@babel/preset-react": "^7.0.0",
        "@babel/register": "^7.0.0",
        "babel-eslint": "^10.0.2",
        "babel-loader": "^8.0.4",
        "commitizen": "^4.2.3",
        "coveralls": "^3.0.6",
        "cross-env": "5.2.0",
        "eslint": "6.1.0",
        "eslint-config-airbnb-base": "14.0.0",
        "eslint-plugin-import": "2.18.2",
        "eslint-watch": "6.0.0",
        "gulp-babel": "8.0.0",
        "gulp-newer": "^1.1.0",
        "gulp-nodemon": "^2.0.6",
        "gulp-sourcemaps": "^2.6.5",
        "istanbul": "1.1.0-alpha.1",
        "jest": "24.8.0",
        "jest-fetch-mock": "2.1.2",
        "jsdom": "15.1.1",
        "validate-commit-msg": "^2.6.1"
    }
}
